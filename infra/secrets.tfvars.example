# Example secrets file for Terraform variables
# Copy this file to secrets.tfvars and fill in your actual values
# DO NOT commit secrets.tfvars to version control

# Google Cloud Project Configuration
project_id  = "your-gcp-project-id"
region      = "us-central1"
environment = "dev"  # dev, staging, or prod

# LLM API Keys for Multi-Model Consensus (GPT-4o, Claude-3.5, grok-3)
openai_api_key     = "sk-your-openai-api-key"
anthropic_api_key  = "your-anthropic-api-key"
grok_api_key       = "your-grok-api-key"

# Google Custom Search for Source Discovery
google_search_api_key = "your-google-custom-search-api-key"
google_search_cx      = "your-custom-search-engine-id"

# Pipeline Client Authentication
auth0_domain   = "your-auth0-domain"
auth0_audience = "your-auth0-audience"
allowed_origins = ["http://localhost:5173"]
